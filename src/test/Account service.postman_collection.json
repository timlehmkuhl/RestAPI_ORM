				{
					"listen": "test",
					"script": {
						"id": "675c4b37-800c-4519-accd-5cb9dc0c2827",
						"exec": [
							"pm.test(\"Post account\", function () {",
							"    pm.response.to.have.status(201);",
							"    var json = pm.response.json();",
							"    pm.expect(json.owner).to.eql(\"A\");",
							"    pm.expect(json.entries.length).to.eq(2);",
							"    ",
							"    pm.globals.set(\"id\", json.id);",
							"    pm.globals.set(\"entriesLength\", json.entries.length); ",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Accept",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\"owner\": \"A\", \"entries\": [{\"value\": 10}, {\"value\": 20}]}"
				},
